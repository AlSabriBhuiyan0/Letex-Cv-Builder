{
  "name": "latex-cv-builder",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "cd client && npm install --legacy-peer-deps && set NODE_OPTIONS=--openssl-legacy-provider && npm run build",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "cd client && npm install"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "latex-cv-builder": "file:",
    "mongoose": "^8.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-latex-next": "^2.2.0"
  },
  "devDependencies": {
    "mocha": "^9.0.0"
  }
}
